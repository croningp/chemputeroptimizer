<Synthesis>

    <Hardware>
      <Component
        id="reactor"
        type="reactor"
      />
    </Hardware>

    <Reagents>
      <Reagent
        name="water"
      />
      <Reagent
        name="methanol"
      />
    </Reagents>

    <Procedure>
      <Add
        reagent="water"
        vessel="reactor"
        volume="30 mL"
      />
      <Stir
        vessel="reactor"
        time="30 secs"
      />
      <DiluteSample
        vessel="reactor"
        sample_volume="3 mL"
        dilution_volume="10 mL"
        dilution_solvent="methanol"
      />
    </Procedure>

  </Synthesis>
