<Synthesis>

    <Hardware>
      <Component
        id="reactor"
        type="reactor"
      />
    </Hardware>

    <Reagents>
      <Reagent
        name="water"
      />
      <Reagent
        name="methanol"
      />
    </Reagents>

    <Procedure>
      <Add
        reagent="water"
        vessel="reactor"
        volume="30 mL"
      />
      <Stir
        vessel="reactor"
        time="30 secs"
      />
      <InjectSample
        sample_vessel="reactor"
        sample_volume="4 mL"
        target_vessel="flask_sample_2"
      />
    </Procedure>

  </Synthesis>
