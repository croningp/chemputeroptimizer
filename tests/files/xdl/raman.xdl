<Synthesis>

  <Hardware>

    <Component

      id="filter"

      type="filter" />

  </Hardware>

  <Reagents>
    <Reagent
      name="copper(I)iodide" />
    <Reagent
      name="distilled water" />
    <Reagent
      name="hydriodic acid" />
    <Reagent
      name="propiolic acid" />
  </Reagents>

  <Procedure>
    <Add
      reagent="hydriodic acid"
      vessel="filter"
      volume="40 mL"
      stir="False" />
    <Add
      reagent="propiolic acid"
      vessel="filter"
      volume="10 mL"
      time="60 secs"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="100°C"
      time="30 mins" />
    <HeatChillToTemp
      vessel="filter"
      temp="28°C"
      active="False" />
    <HeatChill
      vessel="filter"
      temp="25°C"
      time="15 mins" />
    <FinalAnalysis
      method='Raman'
      vessel='filter' />
    <Filter
      vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="distilled water"
      volume="70 mL"
      repeat="3" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>